{
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.autocomplete": "default",
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/../users",
        "${workspaceFolder}/../drivers/stm32g4xx-hal-driver/Inc",
        "${workspaceFolder}/../drivers/stm32g4xx-hal-driver/Inc/Legacy",
        "${workspaceFolder}/../drivers/cmsis-device-g4/Include",
        "${workspaceFolder}/../utilities",
        "${workspaceFolder}/../utilities/math",
        "${workspaceFolder}/../utilities/filter",
        "${workspaceFolder}/../devices/inc",
        "${workspaceFolder}/../modules",
        "${workspaceFolder}/../drivers/bsp/stm32/hal",
        "${workspaceFolder}/../drivers/bsp/stm32/ll",
        "${workspaceFolder}/../middlewares/RTT",
        "${workspaceFolder}/../test",
        "${workspaceFolder}/../middlewares/proto",
        "${workspaceFolder}/../applications"
    ],
    "C_Cpp.default.defines": [
        "USE_HAL_DRIVER",
        "STM32G474xx",
        "USE_FULL_LL_DRIVER"
    ],
    "C_Cpp.default.cStandard": "c99",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.intelliSenseMode": "clang-arm",
    "files.associations": {
        "*.h": "c",
        "*.c": "c"
    },
    "C_Cpp.default.browse": {
        "path": [
            "${workspaceFolder}/.."
        ],
        "limitSymbolsToIncludedHeaders": false
    }
}

